{
    "manifestVersion": 1,
    "id": "MachineIdentityExtension",
    "name": "Machine Identity Extension",
    "version": "0.1.0",
    "publisher": "GregBrownstein",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Simplify key generation and enrollment of machine identities (also known as SSL/TLS certificates and keys)",
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "venafi",
        "ssl",
        "tls",
        "certificate",
        "certificates",
        "key",
        "keys"
    ],
    "screenshots": [

    ],
    "icons": {
        "default": "images/ven_circlev_logo.png"
    },
    "content": {
        "details": {
            "path": "extension/readme.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/gdbarron/MachineIdentityExtension"
    },
    "files": [
        {
            "path": "extension/bin"
        },
        {
            "path": "extension/node_modules/azure-pipelines-task-lib"
        },
        {
            "path": "extension/node_modules/vss-web-extension-sdk"
        },
        {
            "path": "extension/images/icon.png"
        },
        {
            "path": "extension/index.js"
        },
        {
            "path": "extension/task.json"
        }
    ],
    "galleryFlags": [
        "Preview"
    ],
    "contributions": [
        {
            "id": "MachineIdentityExtension",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "extension",
                "icon": "extension/images/icon.png"
            }
        }
    ]
}