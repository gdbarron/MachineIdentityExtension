{
    "manifestVersion": 1,
    "id": "MachineIdentityExtension",
    "name": "Machine Identity Extension",
    "version": "0.1.0",
    "publisher": "GregBrownstein",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Create, retrieve, and renew certificates with Venafi Trust Protection Platform (TPP) or Venafi Cloud utilizing VCert.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/ven_circlev_logo.png"
    },
    "content": {
        "details": {
            "path": "buildAndReleaseTask/readme.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/gdbarron/MachineIdentityExtension"
    },
    "files": [
        {
            "path": "buildAndReleaseTask/bin"
        },
        {
            "path": "buildAndReleaseTask/node_modules/azure-pipelines-task-lib"
        },
        {
            "path": "buildAndReleaseTask/node_modules/vss-web-extension-sdk"
        },
        {
            "path": "buildAndReleaseTask/icon.png"
        },
        {
            "path": "buildAndReleaseTask/index.js"
        },
        {
            "path": "buildAndReleaseTask/task.json"
        }
    ],
    "galleryFlags": [
        "Preview"
    ],
    "contributions": [
        {
            "id": "MachineIdentityExtension",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ]
}